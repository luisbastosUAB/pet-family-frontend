{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
	"headers": [{
                "source": "/dist/**",
                "headers": [{
                    "key": "Cache-Control",
                    "value": "public, max-age=0"
                }]
            }]
  }
}